<section id='title'>
  <h1>All Items</h1>
</section>

<% @items.each do |item| %>
  <section id='item-<%= item.id %>' class='item'>
    <h2><%= item.name %></h2>
    <h3><%= item.description %></h3>
    <h3><%= item.price %></h3>
    <h3><%= item.image %></h3>
    <h3><%= item.active %></h3>
    <h3><%= item.inventory %></h3>
    <h3><%= item.merchant.name %></h3>
  </section>
<% end %>
