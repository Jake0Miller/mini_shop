<%= form_with(model: @merchant) do |form| %>
  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name, id: :merchant_name %>
  </div><br/>

  <div class="field">
    <%= form.label :address %>
    <%= form.text_field :address, id: :merchant_address %>
  </div><br/>

  <div class="field">
    <%= form.label :city %>
    <%= form.text_field :city, id: :merchant_city %>
  </div><br/>

  <div class="field">
    <%= form.label :state %>
    <%= form.text_field :state, id: :merchant_state %>
  </div><br/>

  <div class="field">
    <%= form.label :zip %>
    <%= form.text_field :zip, id: :merchant_zip %>
  </div><br/>

  <div class="field">
    <%= form.submit %>
  </div>
<% end %>
