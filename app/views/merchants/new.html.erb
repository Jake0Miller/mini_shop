<h1>New Merchant</h1>

<%= form_tag(new_merchant_path) do %>
  <div class="field">
    <%= label_tag :name %>
    <%= text_field_tag :name, @merchant.name %>
  </div><br/>

  <div class="field">
    <%= label_tag :address %>
    <%= text_field_tag :address, @merchant.address %>
  </div><br/>

  <div class="field">
    <%= label_tag :city %>
    <%= text_field_tag :city, @merchant.city %>
  </div><br/>

  <div class="field">
    <%= label_tag :state %>
    <%= text_field_tag :state, @merchant.state %>
  </div><br/>

  <div class="field">
    <%= label_tag :zip %>
    <%= text_field_tag :zip, @merchant.zip %>
  </div><br/>

  <div class="field">
    <%= submit_tag("Submit") %>
  </div>
<% end %>
