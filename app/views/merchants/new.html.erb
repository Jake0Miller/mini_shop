<h1 class='title'>New Merchant</h1>

<%= form_tag(new_merchant_path) do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %><br/>
  <br/>
  <%= label_tag :address %>
  <%= text_field_tag :address, @merchant.address %><br/>
  <br/>
  <%= label_tag :city %>
  <%= text_field_tag :city, @merchant.city %><br/>
  <br/>
  <%= label_tag :state %>
  <%= text_field_tag :state, @merchant.state %><br/>
  <br/>
  <%= label_tag :zip %>
  <%= text_field_tag :zip, @merchant.zip %></br>
  <br/>
  <%= submit_tag("Submit") %>
<% end %>
